#include <stdio.h>

int main() {
    int n, i;
    int a = 0, b = 1, c;

    printf("Enter number of terms: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Invalid number of terms\n");
    } else if (n == 1) {
        printf("Fibonacci Series: 0");
    } else {
        printf("Fibonacci Series: 0 1 ");
        for (i = 3; i <= n; i++) {
            c = a + b;
            printf("%d ", c);
            a = b;
            b = c;
        }
    }

    return 0;
}
